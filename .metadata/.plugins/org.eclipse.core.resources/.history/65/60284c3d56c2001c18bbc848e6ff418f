package mypackage;

import java.util.Scanner;

public class Reading 
{

	public static void main(String[] args) 
	{
		
        String name;
        Scanner obj = new Scanner(System.in);
        
        System.out.println("Enter your Name:");
        name =obj.next();
        System.out.println("you entered:+name");
        
        
	}

}
